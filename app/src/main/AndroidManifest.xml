<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="net.middledleeast.tamm">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:icon">
        <activity android:name=".model.PaymentActivityFragment"></activity>
        <!-- <activity android:name=".activities.UsersFree"></activity> -->
        <activity
            android:name=".activities.RenewAccount"
            android:label="@string/title_activity_renew_account"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activities.PassengerDeparture" />
        <activity android:name=".activities.Passenger_inform" />
        <activity android:name=".activities.FlightDetails" />
        <activity android:name=".activities.PassengerInformation" />
        <activity android:name=".activities.FlightsSummary" />
        <activity android:name=".activities.PaymentFlights" />
        <activity android:name=".activities.Proceedbeybey">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.FlightTamm" />
        <activity android:name=".activities.PaymentActivity" />
        <activity android:name=".activities.HotelBooking" />
        <activity android:name=".activities.RoomBooked" />
        <activity android:name=".activities.checkroom" />
        <activity android:name=".activities.ChooseBookingDate"></activity>
        <activity android:name=".activities.HotelDetails" />
        <activity android:name=".activities.ChooseHotelActivity" />
        <activity android:name=".activities.WelcomeActivity"></activity>
        <activity android:name=".LanguageActivity" />
        <activity android:name=".activities.FindHotels"></activity>
        <activity android:name=".activities.ConfirmBookingRoom" />
        <activity android:name=".activities.MainActivity" />
        <activity android:name=".model.ReserveRoom" />
        <activity android:name=".activities.SplashActivity">

        </activity>
    </application>

</manifest>